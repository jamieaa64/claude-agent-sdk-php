{
  "name": "jamieaa64/claude-agent-sdk-php",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Claude\\AgentSdk\\": "src/Claude/AgentSdk/"
    },
    "files": [
      "src/Claude/AgentSdk/Types/Message.php",
      "src/Claude/AgentSdk/Types/Blocks.php",
      "src/Claude/AgentSdk/Types/ToolPermissionContext.php",
      "src/Claude/AgentSdk/Types/PermissionResultAllow.php",
      "src/Claude/AgentSdk/Types/PermissionResultDeny.php",
      "src/Claude/AgentSdk/McpSdk.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Claude\\AgentSdk\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "symfony/process": "^6.0 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  }
}
